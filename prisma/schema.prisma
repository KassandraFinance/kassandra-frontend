generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model sharedImageFund {
  id    String @id
  image String
}

model User {
  walletAddress String  @id @map("wallet_address")
  nickname      String? @unique
  twitter       String?
  website       String?
  telegram      String?
  discord       String?
  description   String?
  image         String?
}
